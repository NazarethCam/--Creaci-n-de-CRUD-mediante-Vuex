td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {};\r\n  },\r\n  mounted() {\r\n    this.$store.dispatch(\"getData\");\r\n  },\r\n  computed: {\r\n    getPosts() {\r\n      return this.$store.state.posts;\r\n    },\r\n    getLoader() {\r\n      return this.$store.state.loading;\r\n    },\r\n  },\r\n  methods: {\r\n    deletePost(id) {\r\n      axios\r\n        .delete(`https://jsonplaceholder.typicode.com/posts/${id}`)\r\n        .then(() => {\r\n          alert(`Se ha eliminado el post con id ${id}`);\r\n          this.$store.dispatch(\"getData\");\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n"]}]}